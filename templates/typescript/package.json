    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vkontakte/eslint-config": "^3.1.0",
    "@vkontakte/prettier-config": "^0.1.0",
